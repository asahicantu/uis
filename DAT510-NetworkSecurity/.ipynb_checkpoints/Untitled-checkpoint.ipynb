    "sdes = SimplifiedDES()\n",
    "sdes\n",
    "bin = BinaryStrings()\n",
    "P = [bin(str(randint(0, 1))) for i in range(8)]\n",
    "K = sdes.random_key()\n",
    "C = sdes.encrypt(P, K)\n",
    "plaintxt = sdes.decrypt(C, K)\n",
    "plaintxt == P"
   ]
